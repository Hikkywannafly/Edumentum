{
  "name": "Edumentum",
  "version": "0.1.0",
  "private": true,
  "description": "A sleek and minimal Next.js starter template, optimized for performance and rapid development.",
  "author": "Accessdenied",
  "keywords": [
    "nextjs",
    "react",
    "boilerplate",
    "template",
    "starter",
    "tailwindcss",
    "minimal",
    "sleek",
    "clean",
    "lightweight",
    "fast",
    "modern",
    "optimized",
    "developer-friendly"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "preview": "next build && next start",
    "lint": "biome lint --write .",
    "lint:fix": "biome lint --write . --unsafe",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "commit": "git cz",
    "prepare": "husky install",
    "build:prepare": "pnpm run typecheck && pnpm run lint && pnpm run format && pnpm run check && pnpm run build"
  },
  "dependencies": {
